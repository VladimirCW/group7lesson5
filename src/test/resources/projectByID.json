{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Project by ID",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "color": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "comment_count": {
      "type": "number"
    },
    "shared": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "sync_id": {
      "type": "number"
    },
    "inbox_project": {
      "type": "boolean"
    },
    "url": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "color"
  ]
}